{
  "repository": {},
  "description": " ",
  "license": "MIT",
  "dependencies": {
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html",
    "phoenix_live_view": "file:../deps/phoenix_live_view"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "tailwindcss -c tailwind.config.js -i css/app.css -o ../priv/static/assets/app.css --minify",
    "build:js": "esbuild js/app.js --bundle --outdir=../priv/static/assets --external:/fonts/* --external:/images/*",
    "deploy": "npm run build",
    "watch": "npm run watch:css & npm run watch:js",
    "watch:css": "tailwindcss -c tailwind.config.js -i css/app.css -o ../priv/static/assets/app.css --watch",
    "watch:js": "esbuild js/app.js --bundle --outdir=../priv/static/assets --external:/fonts/* --external:/images/* --watch"
  },
  "devDependencies": {
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "@tailwindcss/forms": "^0.5.3",
    "esbuild": "^0.19.0"
  }
}